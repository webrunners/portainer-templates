[
  {
  "type": "stack",
  "title": "Runner",
  "description": "Custom GitLab Runner",
  "repository": {
      "url": "https://github.com/webrunners/gitlab-runner-service",
      "stackfile": "runner.yml"
    },
  "note": "You must put TOKEN into a docker secret named like SERVICE.",
  "env": [
      {
      "name": "SERVICE",
      "label": "SERVICE (Name of the secret to read from)"
      },
      {
      "name": "EXECUTOR",
      "label": "EXECUTOR ([shell|docker] Default: shell)"
      },
      {
      "name": "TAG_LIST",
      "label": "TAG_LIST (Tag this runner. Comma separated)"
      },
      {
      "name": "URL",
      "label": "URL (GitLab ci url. Commonly: GitLab-URL/ci)"
      }
    ]
  },
  {
  "type": "stack",
  "title": "WhoamEye",
  "description": "Service Example",
  "repository": {
      "url": "https://github.com/webrunners/gitlab-runner-service",
      "stackfile": "teststack.yml"
    },
  "env": [
      {
      "name": "DOMAIN",
      "label": "DOMAIN"
      },
      {
      "name":  "SERVICE_PORT",
      "label": "SERVICE_PORT"
      }
    ]
  }
