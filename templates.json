[
  {
  "type": "stack",
  "title": "Runner",
  "description": "Custom GitLab Runner",
  "repository": {
      "url": "https://github.com/webrunners/gitlab-runner-service",
      "stackfile": "runner.yml"
    },
  "note": "You should put at least the TOKEN into a docker secret named like SERVICE. Otherwise the token would be clear written in docker inspect.",
  "env": [
      {
      "name": "DESCRIPTION",
      "label": "DESCRIPTION (alt name)"
      },
      {
      "name":  "EXECUTOR",
      "label": "EXECUTOR (shell|docker. default shell)"
      },
      {
      "name": "SERVICE",
      "label": "SERVICE (name of secret)"
      },
      {
      "name": "TAG_LIST",
      "label": "TAG_LIST (tag this runner. comma separated)"
      },
      {
      "name": "TOKEN",
      "label": "TOKEN (put this into secret)"
      },
      {
      "name": "URL",
      "label": "URL (gitlab ci url)"
      }
    ]
  },
  {
  "type": "stack",
  "title": "WhoamEye",
  "description": "Service Example",
  "repository": {
      "url": "https://github.com/webrunners/gitlab-runner-service",
      "stackfile": "teststack.yml"
    },
  "env": [
      {
      "name": "DOMAIN",
      "label": "DOMAIN"
      },
      {
      "name": "REPLICAS",
      "label": "REPLICAS"
      },
      {
      "name":  "SERVICE_PORT",
      "label": "SERVICE_PORT"
      }
    ]
  }
]