[
  {
  "type": "stack",
  "title": "Runner",
  "description": "Custom GitLab Runner",
  "repository": {
      "url": "https://github.com/webrunners/gitlab-runner-service",
      "stackfile": "runner.yml"
    },
  "note": "First create a docker secret, named like SERVICE. You must define TOKEN env variable in there.",
  "env": [
      {
      "name": "SERVICE",
      "label": "Should be the same as 'NAME'"
      },
      {
      "name": "EXECUTOR",
      "label": "EXECUTOR ([shell|docker] Default: shell)"
      },
      {
      "name": "TAG_LIST",
      "label": "TAG_LIST (Tag this runner. Comma separated)"
      },
      {
      "name": "URL",
      "label": "URL (GitLab ci url. Commonly: GitLab-URL/ci)"
      }
    ]
  },
