[{
  "type": "container",
  "title": "Runner",
  "description": "Custom GitLab Runner",
  "image": "webrunners/gitlab-runner-service:latest",
  "env": [
    {
    "name": "DESCRIPTION",
    "label": "alt name"
    },
    {
    "name":  "EXECUTOR",
    "label": "shell|docker. default shell"
    },
    {
    "name": "SERVICE",
    "label": "name of secret. secret could be used as env file."
    },
    {
    "name": "TAG_LIST",
    "label": "tag this runner. comma separated"
    },
    {
    "name": "TOKEN",
    "label": "projects runner token"
    },
    {
    "name": "URL",
    "label": "gitlab ci url"
    }
  ]
}]